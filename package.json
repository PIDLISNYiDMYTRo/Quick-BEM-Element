{
   "name": "quick-bem-element",
   "displayName": "Quick BEM Element",
   "description": "An efficient VSCode extension for quickly inserting BEM elements into your HTML code, streamlining your workflow and ensuring consistent BEM methodology application.",
   "version": "1.1.0",
   "publisher": "DmytroPidlisnyi",
   "engines": {
      "vscode": "^1.75.0"
   },
   "main": "./src/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "quick-bem-element.insertElement",
            "title": "Quick BEM Element"
         }
      ],
      "keybindings": [
         {
            "command": "quick-bem-element.insertElement",
            "key": "alt+q",
            "when": "editorTextFocus",
            "title": "Insert BEM Element"
         }
      ]
   },
   "activationEvents": [
      "onCommand:quick-bem-element.insertElement"
   ],
   "dependencies": {},
   "repository": {
      "type": "git",
      "url": "https://github.com/PIDLISNYiDMYTRo/Quick-BEM-Element"
   },
   "bugs": {
      "url": "https://github.com/PIDLISNYiDMYTRo/Quick-BEM-Element/issues"
   },
   "homepage": "https://github.com/PIDLISNYiDMYTRo/Quick-BEM-Element#readme",
   "icon": "images/icon.png",
   "categories": [
      "Other",
      "Programming Languages",
      "Snippets"
   ],
   "keywords": [
      "BEM",
      "HTML",
      "VSCode",
      "snippets",
      "block",
      "element",
      "modifier"
   ]
}
